<?xml version="1.0"?>
<robot name="KWJBot">
    <link name="base_footprint"/>
   
    <joint name="base_joint" type="fixed">
       <parent link="base_footprint"/>
       <child link="base_link"/>
    </joint>
   
    <link name="base_link">
       <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="1" />
        <inertia ixx="0.0" ixy="0" ixz="0" iyy="0.0" iyz="0" izz="0" /> 
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
          <box size="0.155 0.2 0.24"/>
        </geometry>
        <material name="white">
          <color rgba="1 1 1 1"/>
        </material>
      </visual>
    </link>
      
    
</robot>
